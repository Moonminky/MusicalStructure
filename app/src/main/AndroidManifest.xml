<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicalstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/activity_MoodPicker">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FocusActivity"
            android:label="@string/activity_focus" />
        <activity
            android:name=".ChillActivity"
            android:label="@string/activity_chill" />
        <activity
            android:name=".SleepActivity"
            android:label="@string/activity_sleep" />
        <activity
            android:name=".PartyActivity"
            android:label="@string/activity_party" />
        <activity
            android:name=".WorkoutActivity"
            android:label="@string/activity_workout" />
        <activity
            android:name=".WakeUpActivity"
            android:label="@string/activity_wakeUp" />
        <activity
            android:name=".Settings"
            android:label="@string/activity_settings" />
    </application>

</manifest>